package model;

public class User {

	private int id;
	private int age;
	private String name;
	public User(int id, String name) {
		this.id = id;
		this.setName(name);
	}

	public Integer getId() {
		return Integer.valueOf(id);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
}
